public class PresentCardModel {
	private int index;
	private String senderId;
	private String receiverPhoneNum;
	private String imageURL;
	private String barcodeURL;
	private String menu;
	private String content;
	private Date sendDate;
	private Date useDate;
	private int usedFlag;
	
	
	/*** Constructors ***/
	
	public PresentCardModel() {
		super();
	}
	public PresentCardModel(int index, String senderId,
			String receiverPhoneNum, String imageURL, String barcodeURL,
			String menu, String content, Date sendDate, Date useDate, int usedFlag) {
		super();
		this.index = index;
		this.senderId = senderId;
		this.receiverPhoneNum = receiverPhoneNum;
		this.imageURL = imageURL;
		this.barcodeURL = barcodeURL;
		this.menu = menu;
		this.content = content;
		this.sendDate = sendDate;
		this.useDate = useDate;
		this.usedFlag = usedFlag;
	}
	
	

	/*** Getter and Setters ***/
	
	public int getIndex() {
		return index;
	}
	public void setIndex(int index) {
		this.index = index;
	}
	public String getSenderId() {
		return senderId;
	}
	public void setSenderId(String senderId) {
		this.senderId = senderId;
	}
	public String getReceiverPhoneNum() {
		return receiverPhoneNum;
	}
	public void setReceiverPhoneNum(String receiverPhoneNum) {
		this.receiverPhoneNum = receiverPhoneNum;
	}
	public String getImageURL() {
		return imageURL;
	}
	public void setImageURL(String imageURL) {
		this.imageURL = imageURL;
	}
	public String getBarcodeURL() {
		return barcodeURL;
	}
	public void setBarcodeURL(String barcodeURL) {
		this.barcodeURL = barcodeURL;
	}
	public String getMenu() {
		return menu;
	}
	public void setMenu(String menu) {
		this.menu = menu;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public Date getSendDate() {
		return sendDate;
	}
	public void setSendDate(Date sendDate) {
		this.sendDate = sendDate;
	}
	public Date getUseDate() {
		return useDate;
	}
	public void setUseDate(Date useDate) {
		this.useDate = useDate;
	}
	public int getUsedFlag() {
		return usedFlag;
	}
	public void setUsedFlag(int usedFlag) {
		this.usedFlag = usedFlag;
	}
}
